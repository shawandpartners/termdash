{
    "defwidget@terminal": {
        "display:order": -5,
        "icon": "square-terminal",
        "label": "terminal",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "shell"
            }
        }
    },
    "defwidget@claude": {
        "display:order": -4.5,
        "icon": "wand-magic-sparkles",
        "color": "#bc8cff",
        "label": "claude",
        "description": "Launch Claude Code session",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "shell",
                "cmd": "claude",
                "termdash:type": "claude",
                "frame:activebordercolor": "#bc8cff",
                "frame:bordercolor": "#3d2d5c"
            }
        }
    },
